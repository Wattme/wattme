{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prod": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run --config ormconfig.ts && nest start --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate --config ormconfig.ts -n",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run --config ormconfig.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@binance/connector": "^1.5.0",
    "@bugsnag/js": "^7.14.1",
    "@bugsnag/plugin-express": "^7.14.0",
    "@google-cloud/storage": "^5.16.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "*",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/typeorm": "^8.0.2",
    "@sentry/node": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@solana/web3.js": "^1.31.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bitcoin-address-validation": "^2.1.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "cryptr": "^6.0.2",
    "dotenv": "^10.0.0",
    "ethereumjs-tx": "^2.1.2",
    "ethers": "^5.5.1",
    "exact-math": "^2.2.3",
    "file-type": "^17.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mathjs": "^10.0.0",
    "moment": "^2.29.1",
    "myregexp": "^1.0.8",
    "mysql2": "^2.3.3-rc.0",
    "nestjs-http-promise": "^1.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "string-crypto": "^2.0.2",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2",
    "web3": "^1.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/testing": "^8.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^1.7.3",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mailgun-js": "^0.22.12",
    "@types/node": "^16.11.1",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "class-transformer": "^0.4.0",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
